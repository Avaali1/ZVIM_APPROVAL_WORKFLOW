	<u:ObjectPageLayout id="ObjectPageLayout" subSectionLayout="TitleOnLeft" upperCaseAnchorBar="false">
		<u:headerTitle>
			<u:ObjectPageDynamicHeaderTitle>
				<u:expandedHeading>
					<Title text="{i18n>invoiceTitle}-{header>/WIID}:{header>/DOCID}"/>
				</u:expandedHeading>
			</u:ObjectPageDynamicHeaderTitle>
		</u:headerTitle>
		<u:headerContent>
			<layout:HorizontalLayout allowWrapping="true">
				<ObjectAttribute title="{i18n>compCode}" text="{header>/BUKRS}"/>
				<ObjectAttribute title="{i18n>invAmount}" text="{header>/NET_AMOUNT}"/>
				<ObjectAttribute title="{i18n>docNo}" text="{header>/DOCID}"/>
			</layout:HorizontalLayout>
		</u:headerContent>
		<u:sections>
			<u:ObjectPageSection titleUppercase="false" title="{i18n>basicData}">
				<u:subSections>
					<u:ObjectPageSubSection >
						<u:blocks>
							<BasicData:BasicData id="baisc"/>
						</u:blocks>
					</u:ObjectPageSubSection>
				</u:subSections>
			</u:ObjectPageSection>
			<u:ObjectPageSection titleUppercase="false" title="{i18n>lineItem}">
				<u:subSections>
					<u:ObjectPageSubSection >
						<u:blocks>
							<LineItem:LineItem id="LineItem"/>
						</u:blocks>
					</u:ObjectPageSubSection>
				</u:subSections>
			</u:ObjectPageSection>
			<u:ObjectPageSection titleUppercase="false" title="{i18n>history}">
				<u:subSections>
					<u:ObjectPageSubSection >
						<u:blocks>
							<History:History id="History"/>
						</u:blocks>
					</u:ObjectPageSubSection>
				</u:subSections>
			</u:ObjectPageSection>
			<u:ObjectPageSection titleUppercase="false" title="{i18n>comments}">
				<u:subSections>
					<u:ObjectPageSubSection >
						<u:blocks>
							<Comments:Comment id="Comment"/>
						</u:blocks>
					</u:ObjectPageSubSection>
				</u:subSections>
			</u:ObjectPageSection>
			<u:ObjectPageSection titleUppercase="false" title="{i18n>document}">
				<u:subSections>
					<u:ObjectPageSubSection >
						<u:blocks>
							<Documents:Document id="Document"/>
						</u:blocks>
					</u:ObjectPageSubSection>
				</u:subSections>
			</u:ObjectPageSection>
		</u:sections>
	</u:ObjectPageLayout>
